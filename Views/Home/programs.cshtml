                                <div class="caption">
                                    <h5 class="fw-b"><a href="#">Study abroad programs</a></h5>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--END section1-->
            <!--section2-->
            <section class="well1 bg-variant-1">
                <div class="container">
                    <h3>Certificate Programs</h3>
                    <div class="row text-left">
                        <div class="col-sm-4 col-xs-12">
                            <ul class="marked-list">
                                <li><a href="#">Art & Design</a></li>
                                <li><a href="#">Business</a></li>
                                <li><a href="#">Criminal Justice</a></li>
                                <li><a href="#">Culinary / Hospitality</a></li>
                            </ul>
                        </div>
                        <div class="col-sm-4 col-xs-12">
                            <ul class="marked-list">
                                <li><a href="#">Healthcare / Medical</a></li>
                                <li><a href="#">Human / Social Serv.</a></li>
                                <li><a href="#">Legal / Paralegal</a></li>
                                <li><a href="#">Psychology</a></li>
                            </ul>
                        </div>
                        <div class="col-sm-4 col-xs-12">
                            <ul class="marked-list">
                                <li><a href="#">Teaching / Education</a></li>
                                <li><a href="#">Technology / IT</a></li>
                                <li><a href="#">Trade Schools</a></li>
                                <li><a href="#">Online High School</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </section>
            <!--END section2-->
            <!--section3-->
            <section class="well2 parallax" data-url="~/content/images/parallax3.jpg" data-mobile="true" data-color="#131f25" data-speed="3">
                <div class="container">
                    <div class="row">
                        <div class="col-md-8 col sm-12 col-md-offset-2">
                            <h3>Helping students to achieve their goals every year </h3>
                            <p class="h5">Learning online without boundaries</p>
                            <a href="#" class="btn btn-primary btn-md">click here</a>
                        </div>
                    </div>
                </div>
            </section>
            <!--END section3-->


